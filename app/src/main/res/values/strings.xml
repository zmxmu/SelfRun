    <string name="ET_INPUT_CODE">\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020self.driver.find_element_by_id(\"com.example.zhengmin.maidian:id/%1$s\").send_keys(\"%2$s\")\n</string>
    <string name="VIEW_ON_CLICK_CODE">\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020self.driver.find_element_by_id(\"com.example.zhengmin.maidian:id/%1$s\").click()\n</string>
    <string name="LIST_ITEM_CLICK">\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020self.driver.find_element_by_android_uiautomator(\"text(\\\"%1$s\\\")\").click()\n</string>
