    <string name="ACTIVITY_KEY_BACK">\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020self.driver.press_keycode(4)\n</string>
    <string name="ACTIVITY_TOUCH_DOWN">\u0020\u0020\u0020\u0020\u0020\u0020\u0020\u0020TouchAction(self.driver).press(x=%1$d,y=%2$d)</string>
    <string name="ACTIVITY_TOUCH_UP">.wait(%1$d).move_to(x=%2$d,y=%3$d).release().perform()\n</string>
